{
  "version": "0.0.4",
  "license": "MIT",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "module": "lib/index.mjs",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "config": "pnpm install",
    "rollup": "rollup -c",
    "prepare": "husky install",
    "build": "rm -rf lib && pnpm run rollup",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "remove": "rm -rf lib node_modules pnpm-lock.yaml",
    "clean": "pnpm run remove && pnpm run config",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"{src/**/*,App}.{ts,tsx}\"",
    "lint": "pnpm run prettier && pnpm run eslint --fix",
    "lint-watch": "onchange --await-write-finish 5500 \"**/*\" -- pnpm run lint",
    "postpublish": "git fetch & git pull",
    "prepublishOnly": "pnpm run build && pnpm run lint"
  },
  "name": "@bemedev/x-interpret-react",
  "author": "chlbri <bri_lvi@icloud.com>",
  "devDependencies": {
    "@bemedev/x-test": "0.1.1-beta",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-react": "^3.0.0",
    "esbuild": "^0.16.8",
    "eslint": "^8.30.0",
    "husky": "^8.0.2",
    "jsdom": "^20.0.3",
    "onchange": "^7.1.0",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "rollup": "^3.7.4",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "3.2.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "0.25.2"
  },
  "dependencies": {
    "@xstate/immer": "^0.3.1",
    "immer": "^9.0.16",
    "node-fetch": "^3.3.0",
    "xstate": "^4.35.0",
    "zod": "^3.20.2"
  }
}
